{
  "name": "es-calendar",
  "description": "A calendar based on vue3",
  "keywords": [
    "date",
    "calendar",
    "vue-calendar",
    "es-calendar"
  ],
  "homepage": "https://github.com/vangleer/es-calendar",
  "bugs": {
    "url": "https://github.com/vangleer/es-calendar/issues"
  },
  "license": "MIT",
  "author": "huangwantong <965002107@qq.com>",
  "files": [
    "lib"
  ],
  "main": "./lib/index.umd.js",
  "module": "./lib/index.es.js",
  "exports": {
    ".": {
      "import": "./lib/index.es.js",
      "require": "./lib/index.umd.js",
      "types": "./lib/calendar/index.d.ts"
    },
    "./*": "./*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vangleer/es-calendar.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:lib": "vite build --config vite.config.lib.ts",
    "preview": "vite preview"
  },
  "dependencies": {
    "dayjs": "^1.11.7",
    "vue": ">=3.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.15.13",
    "@vitejs/plugin-vue": "^4.1.0",
    "sass": "^1.62.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-plugin-dts": "^2.3.0",
    "vue-tsc": "^1.2.0"
  },
  "peerDependencies": {
    "vue": ">=3.2.0"
  },
  "typings": "lib/calendar/index.d.ts"
}
